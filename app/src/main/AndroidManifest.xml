<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.mx.testrecycleview">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        tools:ignore="GoogleAppIndexingWarning">
        <activity android:name=".cardrecycleview.CardRecycleViewActivity"></activity>
        <activity android:name=".linkagerecyclerView.LinkedageActivity" />
        <activity android:name=".rvadimage.RvAdImageActivity" />
        <activity android:name=".multisuspensionbar.MultiSuspensionBarActivity" />
        <activity android:name=".suspensionbar.SuspensionBarActivity" />
        <activity android:name=".base.FundamentalActivity" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>